    {
      "name": "nodejs demo",
      "cwd": "${workspaceFolder}/nodejs",
      "program": "${workspaceFolder}/nodejs/index.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },

  ]
